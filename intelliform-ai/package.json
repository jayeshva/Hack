{
  "name": "intelliform-ai",
  "version": "1.0.0",
  "description": "AI-powered, multilingual assistant for filling and submitting government and enterprise forms",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "test:formAssistantAgent": "ts-node src/tests/formAssistantAgentTest.ts",
    "test": "jest --testPathPattern \"src/**/*.test.ts\" --transform={\"^.+\\.tsx?$\": \"babel-jest\"}",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "form",
    "assistant",
    "ai",
    "langchain",
    "langraph",
    "aws",
    "bedrock",
    "whisper",
    "speech-to-text",
    "text-to-speech",
    "pdf"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.859.0",
    "@huggingface/transformers": "^3.7.1",
    "@langchain/community": "^0.3.48",
    "@langchain/core": "^0.3.62",
    "@langchain/langgraph": "^0.3.8",
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "faiss-node": "^0.5.1",
    "fs-extra": "^11.3.0",
    "helmet": "^8.1.0",
    "install": "^0.13.0",
    "ioredis": "^5.7.0",
    "langchain": "^0.3.29",
    "langfuse": "^3.38.4",
    "langfuse-langchain": "^3.38.4",
    "langsmith": "^0.3.50",
    "multer": "^2.0.2",
    "nodemon": "^3.1.10",
    "npm": "^11.5.2",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.1.1",
    "ws": "^8.18.3",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-typescript": "^7.27.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}
